{
  "name": "kevinporten",
  "version": "1.3.3",
  "private": false,
  "license": "MIT",
  "description": "My personal homepage",
  "repository": "https://github.com/kporten/kevinporten.git",
  "author": "Kevin Porten <hello@kevinporten.dev>",
  "scripts": {
    "start": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.tsx,.js,.jsx .",
    "hint": "hint ./src",
    "type-check": "tsc --project ./tsconfig.json && tsc --project ./cypress/tsconfig.json",
    "changelog": "auto-changelog --package --template keepachangelog --sort-commits relevance",
    "test": "jest --watch --notify",
    "test:all": "jest --watchAll --coverage --notify",
    "test:ci": "jest --passWithNoTests",
    "e2e": "cypress open",
    "e2e:run": "cypress run --record",
    "e2e:ci": "start-server-and-test start 8000 e2e:run",
    "commit": "git-cz",
    "version": "yarn changelog && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^14.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/pro-duotone-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@icons-pack/react-simple-icons": "^3.10.0",
    "@loadable/component": "^5.13.1",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "framer-motion": "^2.6.13",
    "gatsby": "^2.24.57",
    "gatsby-image": "^2.4.17",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-intl": "^0.3.3",
    "gatsby-plugin-manifest": "^2.4.28",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-source-contentful": "^2.3.45",
    "graphql": "^14.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-particles-js": "^3.3.0",
    "tailwindcss": "^1.8.8"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@testing-library/cypress": "^7.0.0",
    "@testing-library/dom": "^7.24.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/user-event": "^12.1.4",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.13",
    "@types/loadable__component": "^5.13.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/simple-icons": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "auto-changelog": "^2.2.0",
    "babel-jest": "^26.3.0",
    "babel-preset-gatsby": "^0.5.9",
    "commitizen": "^4.2.1",
    "cypress": "^5.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-testing-library": "^3.8.0",
    "eslint-plugin-unicorn": "^21.0.0",
    "hint": "^6.1.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "start-server-and-test": "^1.11.3",
    "typescript": "^4.0.2"
  }
}
